<section class="banner-panel">
    <div class="owl-carousel owl-theme">
        <% if(sliders){ %>
            <% for(var i=1; i< sliders.length; i++) { %>
        <div class="item">
            <img alt="" src="/uploads/slider/<%= sliders[i].image %>">
        </div>
        <% }} %>
    </div>
</section>
<section class="work-panel">
