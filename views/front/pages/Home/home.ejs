<section class="testimosials">
    <div class="container">
        <div class="row">
            <h2 class="testimonials-titel text-center w-100">
                Testimonials
            </h2>

            <div class="testimonilas-box w-100">
                <div class="owl-carousel testimonials owl-theme">
                    <% if(testimonials){ %>
                        <% for(var i=0; i< testimonials.length; i++) { %>
                    <div class="item">
                        <p>
                            <%- testimonials[i].description %>
                        </p>
                    </div>
                    <% }} %>
                </div>
            </div>

        </div>
    </div>
</section>

<section class="faq">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-center">
                <h2 class="faq-titel">
                    Got questions?
                </h2>
                <p>
                    We've got answers!
                </p>
            </div>
            <div class="col-10 mx-auto">
                <div class="accordion" id="faqExample">
                    <% if(qnas){ %>
                        <% for(var i=0; i< qnas.length; i++) { %>
                            <div class="card">
                                <div class="card-header p-2" id="heading<%=i%>">
                                    <h5 class="mb-0">
                                        <button class="btn btn-link" type="button" data-toggle="collapse"
                                            data-target="#collapse<%=i%>" aria-expanded="true"
                                            aria-controls="collapse<%=i%>">
                                            Q: <%= qnas[i].question %>
                                        </button>
                                    </h5>
                                </div>

                                <div id="collapse<%=i%>" class="collapse "
                                    aria-labelledby="heading<%=i%>" data-parent="#faqExample">
                                    <div class="card-body qna-desc">
                                        <%- qnas[i].answer %>
                                    </div>
                                </div>
                            </div>
                    <% }} %>
                </div>

            </div>
        </div>
    </div>
</section>

