<div class="basic-form-area mg-b-40">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-3"></div>

                    <div class="main-warp" id="photodownload"
                        style="background: url('/uploads/sample/<%= theme.subcategoryImage %>') no-repeat;">
                        <!-- background image -->
                        <div class="logo">
                            <a href="#">
                                <img src="/uploads/companyLogo/<%= userInfo.companyLogo %>">
                            </a>
                        </div>
                        <div class="titel-box">
                            <h2>Happy Anniversary</h2>

                        </div>
                        <div class="middle-box">
                            <img src="/uploads/birthdayContact/<%= contact.contactPic %>">
                        </div>

                        <div class="address">
                            <i class="fa fa-map-marker" aria-hidden="true"></i> <%= userInfo.officeAddress %>
                        </div>
                        <div class="bottom-box">
                            <div><i class="fa fa-phone-square" aria-hidden="true"></i> <%= userInfo.mobile %></div>
                            <div><i class="fa fa-envelope" aria-hidden="true"></i> <%= userInfo.email %></div>
                        </div>

                        <div class="service">


                            <div class="service-text">
                                <a class="servicename" href="#">
                                    <h4>web design</h4>
                                </a>

                            </div>

                            <div class="service-text">
                                <a class="servicename" href="#">
                                    <h4>webpage</h4>
                                </a>

                            </div>

                            <div class="service-text">
                                <a class="servicename" href="#">
                                    <h4>web development</h4>
                                </a>

                            </div>
                        </div>


                    </div>


                    <div class="col-lg-3"></div>



                </div>
            </div>
        </div>
    </div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

<script>
    $(document).ready(function () {

        $('#setbtn').on('click', function () {
            var uniqueThemeCode = $('#themecode').val();
            $.ajax({
                url: "http://localhost:9128/temp/review/anniversary/ajax-theme-set",
                type: "POST",
                data: { defaultAnniversaryTheme: uniqueThemeCode },
                dataType: 'json',
                success: function (result) {

                    Toastify({
                        text: result.msg, duration: 3000, style: {
                            background: "linear-gradient(to right, #00b09b, #96c93d)",
                        },
                    }).showToast();

                }, error: function (err) {
                    console.log(err);
                }
            })
        });

    });
</script>