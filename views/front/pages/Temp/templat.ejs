</div>

<div class="data-table-area mg-b-40">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <% for(var i=0; i< theme.length; i++) { %>
                        <div class="col-lg-3">
                            <div class="income-dashone-total shadow-reset nt-mg-b-30">
                                <div class="income-title">
                                    <div class="main-income-head">
                                        <h1>Theme :</h1>
                                        <h6>
                                            <%= theme[i].uniqueCode %>
                                        </h6>
                                       
                                        <div class="main-income-phara">
                                            <p><a href="/temp/review/<%= theme[i].status %>/<%= userInfo.id %>/<%= theme[i].uniqueCode %>"
                                                    class="color-white">Preview</a></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="income-dashone-pro">
                                    <img class="img-fluid" src="/uploads/themes/birthday/<%= theme[i].name %>" style="height:250px;width:250px;"  />
                                </div>
                            </div>
                        </div>
                        <% } %>
                </div>
            </div>
        </div>
    </div>
</div>