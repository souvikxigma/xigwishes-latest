    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcome-list shadow-reset">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="breadcome-heading">
                                <h2>Template</h2>
                            </div>
                            <ul class="breadcome-menu">
                                <li><a href="/">Home</a> <span class="bread-slash">/</span>
                                </li>
                                <li><span class="bread-blod"><a href="/temp">Template</a> </span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
