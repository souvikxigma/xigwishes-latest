<div class="breadcome-area mg-t-40 mg-b-30">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcome-list shadow-reset">
                    <div class="row">
                        <div class="col-lg-10">
                            <div class="breadcome-heading">
                                <h2>Template Review</h2>
                            </div>
                            <ul class="breadcome-menu">
                                <li><a href="/">Home</a> <span class="bread-slash">/</span>
                                </li>
                                <li><span class="bread-blod"><a
                                            href="/temp/review/<%= userInfo.id %>/<%= theme.uniqueCode %>">Review</a>
                                    </span>
                                </li>
                            </ul>
                        </div>
                        <div class="col-lg-1">
                            <button class="btn btn-primary" id="download">Download</button>
                        </div>
                        <div class="col-lg-1">
                            <a href="/temp" class="btn btn-primary">Theme</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="basic-form-area mg-b-40">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-3"></div>
                    <!-- theme start -->
                    <div class="col-lg-6 temp1" id="photo">
                        <div class="widget-text-box card-footer-css"
                            style="background: url('/uploads/themes/birthday/<%= theme.name %>') no-repeat center center/cover;">
                            <div class="row">
                                <div class="col-lg-2">
                                    <div class="like-love-list text-left">
                                        <img src="/uploads/companyLogo/<%= userInfo.companyLogo %>"
                                            class="img-circle circle-border m-b-md card-footer-company-logo"
                                            alt="logo" />
                                    </div>
                                </div>
                                <div class="col-lg-8">
                                    <div class=" like-love-list text-center">
                                        <img src="/uploads/birthdayContact/<%= contact.contactPic %>"
                                            class="img-circle circle-border m-b-md card-footer-logo" alt="logo" />
                                    </div>
                                </div>
                                <div class="col-lg-2">
                                    
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6 text-right">
                                    <h4><i class="fa fa-phone color-white"></i>
                                        <span class="color-white">
                                            <%= userInfo.mobile %>
                                        </span>
                                    </h4>
                                   
                                </div>
                                <div class="col-lg-6 text-left">
                                    <p><i class="fa fa-envelope color-white"></i>
                                        <span class="color-white">
                                            <%= userInfo.email %>
                                        </span>
                                    </p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 text-center">
                                    <p><i class="fa fa-briefcase color-white"></i>
                                        <span class="color-white">
                                            <%= userInfo.officeAddress %>
                                        </span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- theme end -->
                    <div class="col-lg-3"></div>
                </div>
            </div>
        </div>
    </div>
</div>