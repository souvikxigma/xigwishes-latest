<div class="breadcome-area mg-t-40 mg-b-30">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcome-list shadow-reset">
                    <div class="row">
                        <div class="col-lg-10">
                            <div class="breadcome-heading">
                                <h2>Template Review</h2>
                            </div>
                            <ul class="breadcome-menu">
                                <li><a href="/">Home</a> <span class="bread-slash">/</span>
                                </li>
                                <li><span class="bread-blod"><a
                                            href="/temp/review/<%= userInfo.id %>/<%= theme.uniqueCode %>">Review</a>
                                    </span>
                                </li>
                            </ul>
                        </div>
                        <div class="col-lg-1">
                            <button class="btn btn-primary" id="download">Download</button>
                        </div>
                        <div class="col-lg-1">
                            <a href="/temp" class="btn btn-primary">Theme</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="basic-form-area mg-b-40">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-3"></div>
                    <!-- theme start -->
                    <div class="col-lg-6 temp5" id="photo">
                        <div class="main-warp" style="background: url('/uploads/themes/birthday/<%= theme.name %>') no-repeat;" >
                            <div class="logo">
                                <a href="#">
                                    <img src="/uploads/companyLogo/<%= userInfo.companyLogo %>">
                                </a>
                            </div>
                            <div class="titel-box">
                                <h2>Happy Birthday</h2>
                                <p>
                                    This day only comes once a year, so enjoy it!
                                </p>
                            </div>
                            <div class="middle-box">
                                <img src="/uploads/birthdayContact/<%= contact.contactPic %>">
                            </div>
                            <div class="bottom-box">
                               <div><i class="fa fa-phone-square" aria-hidden="true"></i> <%= userInfo.mobile %></div>
                               <div><i class="fa fa-envelope" aria-hidden="true"></i> <%= userInfo.mobile %></div>
                            </div>
                        </div>
                    </div>
                    <!-- theme end -->
                    <div class="col-lg-3"></div>
                </div>
            </div>
        </div>
    </div>
</div>