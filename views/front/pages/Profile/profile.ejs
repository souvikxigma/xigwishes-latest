<div class="basic-form-area mg-b-40">
    <div class="container">


        <div class="row">
            <div class="col-lg-12">
                <div class="sparkline12-list shadow-reset mg-t-30">
                    <div class="sparkline12-hd">
                        <div class="main-sparkline12-hd">
                            <h1>Profile Form</h1>
                            <div class="sparkline12-outline-icon">
                                <span class="sparkline12-collapse-link"><i class="fa fa-chevron-up"></i></span>
                                <span><i class="fa fa-wrench"></i></span>
                                <span class="sparkline12-collapse-close"><i class="fa fa-times"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="sparkline12-graph">
                        <div class="basic-login-form-ad">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="all-form-element-inner">
                                        <%- include('../../includes/alert.ejs') %>
                                            <form action="/user/profile/action" method="POST"
                                                enctype="multipart/form-data">


                                                <div class="form-group-inner">
                                                    <div class="row">
                                                        <div class="col-lg-3">
                                                            <label class="login2 pull-right pull-right-pro">Name</label>
                                                        </div>
                                                        <div class="col-lg-9">
                                                            <input id="name" type="text"
                                                                placeholder="Enter your name..." name="name"
                                                                value="<%= data.name %>" class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group-inner">
                                                    <div class="row">
                                                        <div class="col-lg-3">
                                                            <label
                                                                class="login2 pull-right pull-right-pro">Email</label>
                                                        </div>
                                                        <div class="col-lg-9">
                                                            <input name="email" id="email" type="email"
                                                                value="<%= data.email %>" readonly
                                                                class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group-inner">
                                                    <div class="row">
                                                        <div class="col-lg-3">
                                                            <label class="login2 pull-right pull-right-pro">Company
                                                                Mobile</label>
                                                        </div>
                                                        <div class="col-lg-9">
                                                            <input id="tel" type="tel" placeholder="Enter your name..."
                                                                name="mobile" value="<%= data.mobile %>"
                                                                class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="form-group-inner">
                                                    <div class="row">
                                                        <div class="col-lg-3">
                                                            <label class="login2 pull-right pull-right-pro">Company
                                                                Name</label>
                                                        </div>
                                                        <div class="col-lg-9">
                                                            <input id="cname" type="text"
                                                                placeholder="Enter your name..." name="companyName"
                                                                value="<%= data.companyName %>" class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group-inner">
                                                    <div class="row">
                                                        <div class="col-lg-3">
                                                            <label class="login2 pull-right pull-right-pro">Company
                                                                Logo</label>
                                                        </div>
                                                        <div class="col-lg-9">
                                                            <input id="clogo" type="file"
                                                                placeholder="Enter your name..." name="companyLogo"
                                                                class="form-control" />
                                                            <% if(data.companyLogo){ %> <img class="img-fluid"
                                                                    style="width: 90px;"
                                                                    src="/uploads/companyLogo/<%= data.companyLogo %>">
                                                                <% } %>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="form-group-inner">
                                                    <div class="row">
                                                        <div class="col-lg-3">
                                                            <label class="login2 pull-right pull-right-pro">Home
                                                                Address</label>
                                                        </div>
                                                        <div class="col-lg-9">
                                                            <textarea class="form-control" name="homeAddress"
                                                                id="homeAddress"><%= data.homeAddress %></textarea>

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group-inner">
                                                    <div class="row">
                                                        <div class="col-lg-3">
                                                            <label class="login2 pull-right pull-right-pro">Office
                                                                Address</label>
                                                        </div>
                                                        <div class="col-lg-9">
                                                            <textarea class="form-control" name="officeAddress"
                                                                id="officeAddress"><%= data.officeAddress %></textarea>

                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="form-group-inner">
                                                    <div class="login-btn-inner">
                                                        <div class="row">
                                                            <div class="col-lg-3"></div>
                                                            <div class="col-lg-9">
                                                                <div class="login-horizental cancel-wp pull-left">
                                                                    <button class="btn btn-white"
                                                                        type="reset">Reset</button>
                                                                    <button
                                                                        class="btn btn-sm btn-primary login-submit-cs"
                                                                        id="submitButton" id="submitButton"
                                                                        type="submit">Submit</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>