<div class="main-panel" id="main-panel">
  <!-- Navbar -->
  <%- include('../../includes/navbar.ejs') %>
    <!-- End Navbar -->
    <div class="panel-header panel-header-sm">

    </div>
    <div class="content">
      <div class="row">
        <div class="col-md-12">
          <div class="card">
            <div class="card-header">
              <h5 class="title">Cms Add</h5>
              <a class="btn btn-dark" href="/admin/cms">Back</a>
            </div>
            <div class="card-body">
              <%- include('../../includes/alert.ejs') %>
                <form action="/admin/cms/add/action" method="POST" enctype='multipart/form-data'>
                  <div class="row">

                    <div class="col-md-12">
                      <div class="form-group">
                        <label>Type</label>
                        <select class="form-control" name="type" id="type">
                          <option selected="true" disabled="disabled">Choose Events</option>
                          <option value="about">About Us</option>
                          <option value="contact">Contact Us</option>
                          <option value="support">Support</option>
                          <option value="policy">Privacy Policy</option>
                          <option value="terms">Terms & Condition</option>
                          <option value="cancelation">Cancelation Refund</option>
                        </select>
                      </div>
                    </div>
                    <div class="col-md-12">
                      <div class="form-group">
                        <label>Description</label>
                        <textarea name="description" class="form-control summernote"></textarea>
                      </div>
                    </div>

                  </div>

                  <button class="btn btn-success" type="submit">Submit</button>
                </form>
            </div>
          </div>
        </div>

      </div>
    </div>
    <footer class="footer">
      <div class=" container-fluid ">
        <nav>
          <ul>
