<div class="main-panel" id="main-panel">
    <!-- Navbar -->
    <%- include('../../includes/navbar.ejs') %>
        <!-- End Navbar -->
        <div class="panel-header panel-header-sm">
        </div>
        <div class="content">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title"> All Users</h4>
                            <!-- <a href="/admin/event/add" class="btn btn-info">Add Users</a> -->
                            
                        </div>
                        <div class="card-body">
                            <div class="">
                                <table class="display" id="table_id">
                                    <thead class="table-dark">
                                        <th>
                                            #
                                        </th>
                                        <th>
                                            name
                                        </th>
                                        <th>
                                            email
                                        </th>
                                        <th>
                                            action
                                        </th>
                                        
                                    </thead>
                                    <tbody>
                                        <% for(var i=0; i< data.length; i++) { %>
                                        <tr>
                                            <td>
                                                <%= (i+1) %>
                                            </td>
                                            <td>
                                                <%= data[i].name %>
                                            </td>
                                            <td>
                                                <%= data[i].email %>
                                            </td>
                                            
                                            <td>
                                                <a class="btn btn-success btn-sm" href="/admin/user/birthday/<%=data[i].id%>">View Birthday List</a>
                                                <a class="btn btn-warning btn-sm" href="/admin/user/anniversary/<%=data[i].id%>">View Anniversary List</a>
                                                <!-- <a class="btn btn-danger btn-sm" onclick="return confirm('Are you sure you want to delete this course?')" href="/deletecourse/<%=data[i].id%>" >DELETE</a> -->
                                            </td>
                                            <!-- <td>
                                                
                                                <img class="img-fluid" style="width: 90px;" src="/uploads/themes/<%= data[i].name %>">
                                            </td> -->
                                            
                                        </tr>
                                        <% } %>
                                        
                                        
                                        
                                       
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        
        <footer class="footer">
            <div class=" container-fluid ">

                <div class="copyright" id="copyright">
                    &copy;
                    <script>
                        document.getElementById('copyright').appendChild(document.createTextNode(new Date().getFullYear()))
                    </script>, Designed by <a href="javascript:void(0)">Xigmapro</a>. Coded by <a
                        href="javascript:void(0)">Admin</a>.
                </div>
            </div>
        </footer>
</div>